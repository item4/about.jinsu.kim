  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "lint": "stylelint \"src/{*,*/**}.{ts,tsx}\" && eslint \"src/{*,*/**}.{ts,tsx}\"",
    "format": "stylelint --fix \"src/{*,*/**}.{ts,tsx}\" && eslint --fix \"src/{*,*/**}.{ts,tsx}\" && prettier --write \"**\"",
