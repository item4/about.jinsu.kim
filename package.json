{
  "name": "resume",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@kfonts/nanum-barun-gothic-otf": "^0.1.3",
    "gatsby": "^4.18.0",
    "gatsby-plugin-emotion": "^7.18.0",
    "gatsby-plugin-google-gtag": "^4.18.0",
    "gatsby-plugin-react-helmet": "^5.18.0",
    "gatsby-plugin-sitemap": "^5.18.0",
    "gatsby-plugin-typescript": "^4.18.0",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "typeface-merriweather": "^1.1.13",
    "typeface-merriweather-sans": "^1.1.13",
    "typeface-staatliches": "^1.1.13",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "start": "gatsby develop",
    "build": "gatsby build",
    "lint": "stylelint \"src/{*,*/**}.{ts,tsx}\" && eslint \"src/{*,*/**}.{ts,tsx}\"",
    "format": "stylelint --fix \"src/{*,*/**}.{ts,tsx}\" && eslint --fix \"src/{*,*/**}.{ts,tsx}\" && prettier --write \"**\"",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "src/{*,**/*}.{js,jsx,ts,tsx}": [
      "stylelint --fix",
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": ["prettier --write", "git add"],
    "*.{json,md,css,scss}": ["prettier --write", "git add"]
  }
}
