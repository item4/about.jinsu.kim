{
  "name": "resume",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.6.3",
  "dependencies": {
    "@next/env": "^13.4.19",
    "@swc-node/register": "^1.6.7",
    "@swc/core": "^1.3.83",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "minify-xml": "^4.0.0",
    "next": "^13.4.19",
    "next-sitemap": "^4.2.3",
    "pretendard": "^1.3.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "schema-dts": "^1.1.2",
    "the-new-css-reset": "^1.9.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@cfworker/json-schema": "^1.12.7",
    "@next/eslint-plugin-next": "^13.4.19",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "serve": "^14.2.1"
  },
  "scripts": {
    "build": "yarn build:next && yarn build:sitemap && yarn build:etc",
    "build:next": "next build",
    "build:sitemap": "next-sitemap --config next-sitemap.config.mjs",
    "build:etc": "yarn swc-node src/core/postbuild/index.ts",
    "setup": "yarn setup:husky",
    "setup:husky": "husky install",
    "check": "yarn check:json && yarn check:lint && yarn check:type",
    "check:json": "yarn swc-node src/format/json.ts",
    "check:lint": "next lint",
    "check:type": "tsc --noEmit",
    "dev": "next dev",
    "dev:serve": "serve .out",
    "format": "yarn format:json && yarn format:lint && yarn format:prettier",
    "format:json": "yarn swc-node src/format/json.ts --format",
    "format:lint": "next lint --fix",
    "format:prettier": "prettier --write .",
    "start": "yarn dev",
    "swc-node": "yarn node --loader @swc-node/register/esm"
  }
}
