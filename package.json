{
  "name": "resume",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.1.0",
  "dependencies": {
    "@next/env": "^14.1.0",
    "@swc-node/register": "^1.8.0",
    "@swc/core": "^1.4.2",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "minify-xml": "^4.4.1",
    "next": "^14.1.1",
    "next-sitemap": "^4.2.3",
    "pretendard": "^1.3.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "schema-dts": "^1.1.2",
    "the-new-css-reset": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@cfworker/json-schema": "^1.12.8",
    "@next/eslint-plugin-next": "^14.1.1",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "serve": "^14.2.1"
  },
  "scripts": {
    "build": "yarn build:next && yarn build:sitemap && yarn build:etc",
    "build:next": "next build",
    "build:sitemap": "next-sitemap --config next-sitemap.config.mjs",
    "build:etc": "yarn swc-node src/core/postbuild/index.ts",
    "setup": "yarn setup:husky",
    "setup:husky": "husky install",
    "check": "yarn check:json && yarn check:lint && yarn check:type",
    "check:json": "yarn swc-node src/format/json.ts",
    "check:lint": "next lint",
    "check:type": "tsc --noEmit",
    "dev": "next dev",
    "dev:serve": "serve out",
    "format": "yarn format:json && yarn format:lint && yarn format:prettier",
    "format:json": "yarn swc-node src/format/json.ts --format",
    "format:lint": "next lint --fix",
    "format:prettier": "prettier --write .",
    "start": "yarn dev",
    "swc-node": "yarn node --import @swc-node/register/esm-register"
  }
}
