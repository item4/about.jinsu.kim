{
  "name": "resume",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.8.1",
  "dependencies": {
    "@next/third-parties": "^15.2.4",
    "@swc-node/register": "^1.10.10",
    "@swc/core": "^1.11.16",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "next": "^15.2.4",
    "pretendard": "^1.3.9",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "schema-dts": "^1.1.5",
    "the-new-css-reset": "^1.11.3",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@eslint/compat": "^1.2.8",
    "@eslint/css": "^0.6.0",
    "@eslint/js": "^9.23.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "serve": "^14.2.4",
    "typescript-eslint": "^8.29.0"
  },
  "scripts": {
    "build": "yarn build:next && yarn build:etc",
    "build:next": "next build",
    "build:etc": "yarn swc-node src/core/postbuild/index.ts",
    "setup": "yarn setup:husky",
    "setup:husky": "husky init",
    "check": "yarn check:json && yarn check:lint && yarn check:type",
    "check:json": "yarn swc-node src/format/json.ts",
    "check:lint": "next lint",
    "check:type": "tsc --noEmit",
    "dev": "next dev",
    "dev:serve": "serve out",
    "format": "yarn format:json && yarn format:lint && yarn format:prettier",
    "format:json": "yarn swc-node src/format/json.ts --format",
    "format:lint": "next lint --fix",
    "format:prettier": "prettier --write .",
    "start": "yarn dev",
    "swc-node": "yarn node --import @swc-node/register/esm-register",
    "prepare": "husky"
  }
}
